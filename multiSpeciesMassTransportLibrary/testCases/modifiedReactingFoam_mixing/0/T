FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";


    class volScalarField;
    object T;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 298;

boundaryField
{
    inletAir
    {
         type            fixedValue;
        value           uniform 298;
    }

    inletFuel        
    {
         type            fixedValue;
        value           uniform 298;
    }

    outlet          
    {
          
       type            zeroGradient;
    }


    wallBetween
    {
        type            fixedValue;
	value		uniform 298;
    }

    side
    {
        type            symmetryPlane;
	//type            symmetry;
    }

    down
    {
        type            symmetryPlane;
    }
    
    frontAndBackPlanes
    {
        type            empty;
    }
}

