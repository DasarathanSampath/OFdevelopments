    "(U|Yi|h|k|epsilon)"
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-06;
        relTol          0;
    }

    "(U|Yi|h|k|epsilon)Final"
    {
        $Yi;
        preconditioner  DILU;
        tolerance       1e-06;
        relTol          0;
    }

   "(Yi|O2|N2|CH4)"
   {
	solver PBiCG;
	preconditioner DILU;
	tolerance 1e-06;
	relTol 0.1;
   }

   "(Yi|O2|N2|CH4)Final"
   {	
	$Yi;
	preconditioner DILU;
	tolerance 1e-06;
	relTol 0;
   }

}

PIMPLE
{
    transonic       yes;
    nCorrectors     3;
    nNonOrthogonalCorrectors 0;
    momentumPredictor yes;
}


